{
    "input_shape":[200],
    "batch_size": 128,
    "dataset": "train.p",
    "loss": "sparse_categorical_crossentropy",
    "split": 0.2,
    "optimizer": "adam",
    "lr": 0.001,
    "epochs": 10,
    "layers":[
        {
            "name": "Dense",
            "units": 100,
            "input": "in",
            "activation": "relu",
            "output_shape": [100]
        },
        {
            "name": "Dense",
            "units": 50,
            "input": 0,
            "activation": "relu",
            "output_shape": [50]

        },
        {
            "name": "Dense",
            "units": 4,
            "input": 1,
            "activation": "softmax",
            "output_shape": [4]
        }
    ]
}